// }
int main() {
 std::vector<int> nums = {1,2,3,4,5,6,7};
 int n = nums.size();
 // print the original vector
 for(int i=0;i<n;i++) {
  std::cout<<nums[i]<<" ";
 }
 std::cout<<std::endl;
 
 //reverse the vector from l to r 
 //reverse(nums,3,n-1);
 reverse(nums.begin()+3,nums.end());
 
 // print the reversed vector
 for(int i=0;i<n;i++) {
  std::cout<<nums[i]<<" ";
 }
 std::cout<<std::endl;
 
}
